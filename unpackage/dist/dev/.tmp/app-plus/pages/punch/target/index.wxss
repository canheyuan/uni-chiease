/* 文章场景相关 */
page {
  background: #f7f7f7;
}
/* 打卡目标 */
.punch_target {
  background: #f7f7f7;
}
.punch_time_mdl {
  padding: 50rpx 0;
  text-align: center;
  /*火狐*/
  background: -webkit-linear-gradient(top, #c9f3fd, #c7e0fb);
  /*Safari5.1 Chrome 10+*/
}
.punch_circle {
  width: 410rpx;
  height: 410rpx;
  background: url(../../static/img/circle.64d5c8e7.png) no-repeat;
  background-size: 100% 100%;
  margin: 0 auto;
  color: #fff;
  position: relative;
  padding: 40rpx;
}
.punch_circle_main {
  width: 330rpx;
  height: 330rpx;
  background: #39d9e4;
  padding-top: 40rpx;
  border-radius: 100%;
  -o-transition: all .3s;
  transition: all .3s;
  -webkit-transition: all .3s;
}
.punch_circle .p {
  font-size: 32rpx;
}
.punch_circle .h3 {
  font-size: 160rpx;
  font-weight: bold;
  line-height: 180rpx;
}
.punch_circle .cloud {
  position: absolute;
  width: 135rpx;
}
.punch_circle .cloud01 {
  left: -30rpx;
  top: 20rpx;
  -webkit-animation: cloud 15s linear 0s infinite normal;
          animation: cloud 15s linear 0s infinite normal;
}
.punch_circle .cloud02 {
  right: -60rpx;
  top: 180rpx;
  -webkit-animation: cloud 10s linear 2s infinite normal;
          animation: cloud 10s linear 2s infinite normal;
}
.punch_circle .cloud03 {
  left: -100rpx;
  top: 250rpx;
  width: 80rpx;
  -webkit-animation: cloud 12s linear 0s infinite normal;
          animation: cloud 12s linear 0s infinite normal;
}
/*打卡时间*/
.punch_time_num {
  margin: 60rpx 30rpx 0;
}
.num_list {
  display: box;
  display: -webkit-box;
  margin: 0 6rpx 4rpx;
}
.num_list .li {
  width: 100%;
  box-flex: 1;
  -webkit-box-flex: 1;
  font-size: 24rpx;
  color: #333;
}
.time_list {
  border: 6rpx solid #fff;
  height: 44rpx;
  border-radius: 22rpx;
  display: box;
  display: -webkit-box;
}
.time_list .li {
  width: 100%;
  height: 32rpx;
  box-flex: 1;
  -webkit-box-flex: 1;
  -o-transition: all .3s;
  transition: all .3s;
  -webkit-transition: all .3s;
  position: relative;
  z-index: 1;
}
.time_list .li:nth-child(1) {
  background: #ffc10a;
  border-top-left-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
}
.time_list .li:nth-child(2) {
  background: #35afff;
}
.time_list .li:nth-child(3) {
  background: #ff5555;
}
.time_list .li:nth-child(4) {
  background: #39d9e4;
}
.time_list .li:nth-child(5) {
  background: #f271ea;
}
.time_list .li:nth-child(6) {
  background: #94d070;
  border-top-right-radius: 16rpx;
  border-bottom-right-radius: 16rpx;
}
.time_list .li .ico_gou {
  width: 40rpx;
  height: 30rpx;
  background-position: -30rpx -60rpx;
  -webkit-transform: scale(0.6, 0.6);
      -ms-transform: scale(0.6, 0.6);
          transform: scale(0.6, 0.6);
  vertical-align: top;
  opacity: 0;
  -o-transition: all .3s;
  transition: all .3s;
  -webkit-transition: all .3s;
}
.time_list .li.act {
  -ms-transform: scale(1.1, 1.375);
      transform: scale(1.1, 1.375);
  -webkit-transform: scale(1.1, 1.375);
  z-index: 9;
}
.time_list .li.act:nth-child(1) {
  -webkit-box-shadow: 6rpx 0 24rpx rgba(255, 193, 10, 0.5);
          box-shadow: 6rpx 0 24rpx rgba(255, 193, 10, 0.5);
}
.time_list .li.act:nth-child(2) {
  -webkit-box-shadow: 6rpx 0 24rpx rgba(53, 175, 255, 0.5);
          box-shadow: 6rpx 0 24rpx rgba(53, 175, 255, 0.5);
}
.time_list .li.act:nth-child(3) {
  -webkit-box-shadow: 6rpx 0 24rpx rgba(255, 85, 85, 0.5);
          box-shadow: 6rpx 0 24rpx rgba(255, 85, 85, 0.5);
}
.time_list .li.act:nth-child(4) {
  -webkit-box-shadow: 6rpx 0 24rpx rgba(6, 193, 206, 0.5);
          box-shadow: 6rpx 0 24rpx rgba(6, 193, 206, 0.5);
}
.time_list .li.act:nth-child(5) {
  -webkit-box-shadow: 6rpx 0 24rpx rgba(242, 113, 234, 0.5);
          box-shadow: 6rpx 0 24rpx rgba(242, 113, 234, 0.5);
}
.time_list .li.act:nth-child(6) {
  -webkit-box-shadow: 6rpx 0 24rpx rgba(148, 208, 112, 0.5);
          box-shadow: 6rpx 0 24rpx rgba(148, 208, 112, 0.5);
}
.time_list .li.act .ico_gou {
  opacity: 1;
}
.punch_time_mdl .tip_txt {
  font-size: 26rpx;
  color: #777777;
  line-height: 36rpx;
  margin: 30rpx 30rpx 0;
  padding-top: 20rpx;
  border-top: 2rpx solid #bebebe;
}
.punch_target_btn .li {
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  margin-top: 30rpx;
  position: relative;
}
.punch_target_btn .li .btn {
  display: block;
  height: 100%;
}
.punch_target_btn .li .h4 {
  padding-left: 30rpx;
  font-size: 32rpx;
  color: #242629;
}
.punch_target_btn .li .h4 .icons {
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
  position: relative;
  top: -2rpx;
  margin-right: 15rpx;
}
.punch_target_btn .li .h4 .ico_email {
  background-position: -150rpx 0;
}
.punch_target_btn .li .h4 .ico_day {
  background-position: -200rpx 0;
}
.punch_target_btn .li .span {
  position: absolute;
  right: 65rpx;
  display: inline-block;
  top: 0;
  line-height: 100rpx;
  font-size: 24rpx;
  color: #999;
  text-align: right;
}
.punch_target_btn .li .ico_tri {
  width: 20rpx;
  height: 30rpx;
  position: absolute;
  right: 30rpx;
  top: 35rpx;
  background-position: -70rpx -60rpx;
}
.punch_target_btn .li .on_off_btn {
  position: absolute;
  right: 30rpx;
  top: -4rpx;
}
.gray_txt {
  color: #bebebe;
  font-size: 26rpx;
  text-align: center;
  margin: 20rpx 0;
}
@-webkit-keyframes cloud {
0% {
    -webkit-transform: translate(0, 0);
            transform: translate(0, 0);
}
25% {
    -webkit-transform: translate(20px, 10px);
            transform: translate(20px, 10px);
}
50% {
    -webkit-transform: translate(10px, 15px);
            transform: translate(10px, 15px);
}
75% {
    -webkit-transform: translate(-10px, 10px);
            transform: translate(-10px, 10px);
}
100% {
    -webkit-transform: translate(0, 0);
            transform: translate(0, 0);
}
}
@keyframes cloud {
0% {
    -webkit-transform: translate(0, 0);
            transform: translate(0, 0);
}
25% {
    -webkit-transform: translate(20px, 10px);
            transform: translate(20px, 10px);
}
50% {
    -webkit-transform: translate(10px, 15px);
            transform: translate(10px, 15px);
}
75% {
    -webkit-transform: translate(-10px, 10px);
            transform: translate(-10px, 10px);
}
100% {
    -webkit-transform: translate(0, 0);
            transform: translate(0, 0);
}
}

