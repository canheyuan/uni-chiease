/*--------- 提醒打卡页面 -----------*/
.punch_tip_page {
  background: url(../../static/img/punch_bg02.098144e3.jpg) #69bd7d no-repeat center top;
  background-size: 100% auto;
  width: 100%;
  height: 100%;
  position: relative;
}
.punch_tip_box {
  position: absolute;
  top: 330rpx;
  width: 650rpx;
  left: 50rpx;
  background: rgba(255, 255, 255, 0.5);
  border-radius: 30rpx;
  padding: 25rpx;
}
.punch_tip_main {
  background: #fff;
  border-radius: 16rpx;
  height: 540rpx;
  padding-top: 30rpx;
  text-align: center;
}
.punch_tip_main .title {
  background: url(data:image/png;base64,) no-repeat center top;
  background-size: 100% 100%;
  display: inline-block;
  width: 580rpx;
  height: 100rpx;
  line-height: 80rpx;
  font-size: 36rpx;
  color: #fff;
  margin-bottom: 20rpx;
}
.punch_tip_main .learn_time {
  font-size: 32rpx;
  color: #f66060;
  font-weight: 500;
  margin-bottom: 50rpx;
}
.punch_tip_main .learn_time .span {
  display: inline-block;
  font-size: 120rpx;
  font-weight: bold;
  margin: 0 10rpx;
}
.punch_tip_main .txt {
  font-size: 28rpx;
  color: #e98c00;
  margin-bottom: 15rpx;
}
.punch_tip_main .txt .span {
  display: inline-block;
  margin: 0 10rpx;
}
.punch_tip_main .yellow_btn {
  margin: 0 80rpx;
}

