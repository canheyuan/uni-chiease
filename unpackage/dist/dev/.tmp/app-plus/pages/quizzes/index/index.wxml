<block wx:if="{{subjectData}}"><view><uni-popup class="vue-ref" vue-id="1" type="center" maskClick="{{false}}" data-ref="startPop" bind:__l="__l" vue-slots="{{['default']}}"><view class="start_pop"><image class="top_img _img" src="static/images/start_bg.png" alt></image><view class="h3">Chinese Proficiency Test</view><view class="p">Accurate Testing<view class="_br"></view>Here you are,professional test!<view class="_br"></view>Here you go,scientific teaching!</view><view data-event-opts="{{[['tap',[['startSubject',['$event']]]]]}}" class="btn" bindtap="__e">Start Test</view></view></uni-popup><uni-popup class="vue-ref" vue-id="2" type="center" maskClick="{{false}}" data-ref="pausePop" bind:__l="__l" vue-slots="{{['default']}}"><view class="subject_mask_btn"><view data-event-opts="{{[['tap',[['pauseSubject',['$event']]]]]}}" class="btn" bindtap="__e">继续答题</view><view class="btn">退出</view></view></uni-popup><block wx:if="{{subjectItem}}"><view class="subject_head"><view class="progress" style="{{'width:'+((subjectIndex+1)/5*100+'%')+';'}}" id="progress"></view><view class="play_btn"><view class="icons ico_pause"></view><view class="cirqueBox"><view class="cirqueYellow"></view><view data-ref="cirqueBox" class="{{['vue-ref',(subjectIsShow)?'cirqueGrey':'',(isAnswerIn)?'play':'']}}"></view></view></view><view class="txt_jf"><text class="span">{{subjectIndex+1}}</text>/5</view></view></block><block wx:if="{{subjectItem}}"><view class="{{['subject_mdl',[(subjectIsShow)?'show':'']]}}"><block wx:if="{{subjectItem.questionType==1}}"><view class="subject_item"><view class="tyxt_list"><block wx:for="{{subjectItem.options}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['subjectChoiceFn',[index]]]]]}}" class="{{['li',[(item.isChecked)?'act':'']]}}" bindtap="__e"><image src="{{item.img}}" alt></image><view class="mask"><view class="ico_gou"></view></view></view></block></view><view data-event-opts="{{[['tap',[['subjectPlayAudio',['$0'],['subjectItem.audio']]]]]}}" class="audio_btn audio_btm" bindtap="__e"><image class="img" src="{{imgUrl+'/ico_audio.png'}}" alt></image></view></view></block><block wx:if="{{subjectItem.questionType==4}}"><view class="subject_item"><view class="tyxz_img"><image class="img _img" mode="widthFix" src="{{subjectItem.img}}" alt></image></view><view class="tyxt_answer">{{subjectItem.content}}</view><view data-event-opts="{{[['tap',[['subjectPlayAudio',['$0','$event'],['subjectItem.audio']]]]]}}" class="audio_btn audio_center" bindtap="__e"><image class="img" src="{{imgUrl+'/ico_audio.png'}}" alt></image></view><view class="answer_btn01"><block wx:for="{{subjectItem.options}}" wx:for-item="item" wx:for-index="index"><view data-event-opts="{{[['tap',[['subjectChoiceFn',[index]]]]]}}" class="{{['li',[(!item.isPlayAudio)?'df':''],[(item.isChecked)?'act':'']]}}" bindtap="__e"><text class="span">{{item.content}}</text></view></block></view></view></block><block wx:if="{{subjectItem.questionType==5}}"><view class="{{[(true)?'subject_item':'',(subjectItem.contentNum>=subjectItem.contentArr.length-1)?'subject_act':'']}}"><view class="tyxz_img"><image class="img" mode="widthFix" src="{{subjectItem.img}}" alt></image></view><view class="tyxz_text"><block wx:for="{{subjectItem.contentArr}}" wx:for-item="item" wx:for-index="index"><view class="p">{{''+item.val+''}}<block wx:if="{{index!=subjectItem.contentArr.length-1}}"><text data-event-opts="{{[['tap',[['removeAnswer',[index]]]]]}}" class="span" bindtap="__e">{{''+(item.answerIndex===''?'':subjectItem.options[item.answerIndex].content)+''}}</text></block></view></block></view><view data-event-opts="{{[['tap',[['subjectPlayAudio',['$0','$event'],['subjectItem.audio']]]]]}}" class="audio_btn audio_center" bindtap="__e"><image class="img" src="{{imgUrl+'/ico_audio.png'}}" alt></image></view><view class="answer_btn02 answer_btm"><block wx:for="{{subjectItem.options}}" wx:for-item="item" wx:for-index="index"><view data-event-opts="{{[['tap',[['subjectChoiceFn',[index]]]]]}}" class="{{['li',[(item.isChecked)?'act':'']]}}" bindtap="__e">{{item.content}}</view></block></view><view data-event-opts="{{[['tap',[['affirmAnswer']]]]}}" class="submit_btn" bindtap="__e">Submit</view></view></block><block wx:if="{{subjectItem.questionType==6}}"><view class="subject_item"><view class="tyxz_img"><image class="img" mode="widthFix" src="{{subjectItem.img}}" alt></image></view><view data-event-opts="{{[['tap',[['subjectPlayAudio',['$0','$event'],['subjectItem.audioList']]]]]}}" class="audio_btn audio_center" bindtap="__e"><image class="img" src="{{imgUrl+'/ico_audio.png'}}" alt></image></view><view class="answer_btn03"><block wx:for="{{subjectItem.options}}" wx:for-item="item" wx:for-index="index"><view data-event-opts="{{[['tap',[['subjectChoiceFn',[index]]]]]}}" class="{{['li',[(!item.isPlayAudio)?'df':''],[(item.isChecked)?'act':'']]}}" bindtap="__e"><text class="span">{{item.optionCode}}</text></view></block></view></view></block><block wx:if="{{subjectItem.questionType==7}}"><view class="{{[(true)?'subject_item':'',(subjectItem.answerArr.length==subjectItem.options.length)?'subject_act':'']}}"><view class="tyxz_img"><image class="img _img" mode="widthFix" src="{{subjectItem.img}}" alt></image></view><view data-event-opts="{{[['tap',[['subjectPlayAudio',['$0','$event'],['subjectItem.audio']]]]]}}" class="audio_btn audio_center" bindtap="__e"><image class="img" src="{{imgUrl+'/ico_audio.png'}}" alt></image></view><view class="answer_btn02" style="position:static;"><block wx:for="{{subjectItem.answerArr}}" wx:for-item="item" wx:for-index="index"><view data-event-opts="{{[['tap',[['removeAnswer',[index]]]]]}}" class="li" bindtap="__e">{{subjectItem.options[item].content}}</view></block></view><view class="answer_btn02 answer_btm"><block wx:for="{{subjectItem.options}}" wx:for-item="item" wx:for-index="index"><view data-event-opts="{{[['tap',[['subjectChoiceFn',[index]]]]]}}" class="{{['li',[(item.isChecked)?'act':'']]}}" bindtap="__e">{{item.content}}</view></block></view><view data-event-opts="{{[['tap',[['affirmAnswer',[$root.g0]]]]]}}" class="submit_btn" bindtap="__e">Submit</view></view></block></view></block></view></block>