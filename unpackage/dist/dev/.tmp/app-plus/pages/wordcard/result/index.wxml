<block wx:if="{{wordParentData}}"><view class="topic-result"><image class="head-img" src="{{imgUrl+'/word/word-result-img.png'}}" alt></image><view class="title"><view class="h3"><view class="span">{{langData.topicResult.desTitle[lan]}}</view></view></view><view class="p">{{''+(langData.topicResult.desText1[lan]+wordParentData.nameMulti[lan]+langData.topicResult.desText2[lan]+wordParentData.subitem+langData.topicResult.desText3[lan])+''}}</view><block wx:if="{{source=='app'}}"><view data-event-opts="{{[['tap',[['shareFn',['$event']]]]]}}" class="share-btn" bindtap="__e">{{langData.topicResult.shareBtn[lan]}}</view></block><block wx:if="{{source=='app'}}"><navigator class="learn-more-btn" url="{{'../index/index?category='+category}}">{{langData.topicResult.studyBtn[lan]}}</navigator></block><block wx:if="{{source=='h5'}}"><view data-event-opts="{{[['tap',[['downlaodPopFn',['$event']]]]]}}" class="share-btn" bindtap="__e">{{langData.topicResult.downLoadBtn[lan]}}</view></block><uni-popup class="vue-ref" vue-id="1" type="bottom" data-ref="dlPopup" bind:__l="__l" vue-slots="{{['default']}}"><view class="download_pop_btn"><view class="tit">{{langData.topicResult.popTitle[lan]}}</view><block wx:if="{{system=='pc'}}"><view data-event-opts="{{[['tap',[['gotoDownload',['$0'],['dlUrl.app']]]]]}}" class="a" bindtap="__e"><view class="word-icons ico_01"></view>App Store</view></block><view data-event-opts="{{[['tap',[['gotoDownload',['$0'],['dlUrl.GP']]]]]}}" class="a" bindtap="__e"><view class="word-icons ico_02"></view>Google Play</view><view data-event-opts="{{[['tap',[['gotoDownload',['$0'],['dlUrl.myapp']]]]]}}" class="a" bindtap="__e"><view class="word-icons ico_03"></view>MyApp.com</view></view><view data-event-opts="{{[['tap',[['popupShowFn',['dlPopup',false]]]]]}}" class="download_pop_close" bindtap="__e">{{langData.topicResult.closeBtnText[lan]}}</view></uni-popup></view></block>