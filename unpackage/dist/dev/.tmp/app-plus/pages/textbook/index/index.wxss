.tb_page {
  width: 100%;
}
.tb_page_ctn {
  overflow: auto;
  position: relative;
  -webkit-transform: translateY(0rpx);
      -ms-transform: translateY(0rpx);
          transform: translateY(0rpx);
}
.fixed_btn {
  position: fixed;
  right: 0;
  top: 50%;
  z-index: 100;
  width: 88rpx;
  margin-top: -240rpx;
}
.fixed_btn .btn {
    display: inline-block;
    width: 88rpx;
    height: 230rpx;
    padding: 50rpx 20rpx 0 40rpx;
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
.fixed_btn .btn .ico {
      dislay: inline-block;
      width: 28rpx;
      height: 28rpx;
      position: relative;
}
.fixed_btn .btn .text {
      display: inline-block;
      width: 28rpx;
      line-height: 28rpx;
      font-size: 26rpx;
      color: #1788D2;
}
.fixed_btn .prev_btn {
    background-image: url(data:image/png;base64,);
}
.fixed_btn .next_btn {
    background-image: url(data:image/png;base64,);
    margin-top: -20rpx;
    padding-top: 54rpx;
}
.fixed_btn .disabled .ico {
    -webkit-filter: grayscale(1);
    opacity: .5;
}
.fixed_btn .disabled .text {
    color: #ccc;
}
.pull_tip {
  font-size: 26rpx;
  color: #1788D2;
  height: 92rpx;
  line-height: 52rpx;
  text-align: center;
  padding: 20rpx 0;
  width: 100%;
}
.pull_tip .text {
    display: inline-block;
}
.pull_tip .ico {
    display: inline-block;
    width: 52rpx;
    height: 52rpx;
    vertical-align: top;
    margin-left: 20rpx;
}
.pull_tip.down {
  top: -80rpx;
}
.pull_tip.down .ico {
    -webkit-transform: rotate(180deg);
        -ms-transform: rotate(180deg);
            transform: rotate(180deg);
}
.play_box {
  position: fixed;
  right: 0;
  top: 40rpx;
  z-index: 99;
  display: inline-block;
  padding: 8rpx 0 8rpx 10rpx;
  width: 184rpx;
  height: 80rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  background: url(data:image/png;base64,) no-repeat center top;
  background-size: 100% 100%;
}
.play_box .ico_voice {
    width: 64rpx;
    height: 64rpx;
    float: left;
}
.play_box .unplay {
    display: block;
    overflow: hidden;
    text-align: center;
    color: #fff;
    line-height: 64rpx;
    font-weight: bold;
}
.play_box .playing {
    overflow: hidden;
}
.playing {
  height: 64rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
      -ms-flex-align: end;
          align-items: flex-end;
  padding: 10rpx 15rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.playing__bar {
  display: inline-block;
  background: white;
  width: 10rpx;
  height: 100%;
  -webkit-animation: up-and-down 1.3s ease infinite alternate;
  animation: up-and-down 1.3s ease infinite alternate;
}
.playing__bar1 {
  height: 50%;
}
.playing__bar2 {
  height: 20%;
  -webkit-animation-delay: -2.4s;
  animation-delay: -2.4s;
}
.playing__bar3 {
  height: 85%;
  -webkit-animation-delay: -5s;
  animation-delay: -5s;
}
.playing__bar4 {
  height: 70%;
  -webkit-animation-delay: -3.7s;
  animation-delay: -3.7s;
}
.playing__bar3 {
  height: 35%;
  -webkit-animation-delay: -6s;
  animation-delay: -6s;
}
@-webkit-keyframes up-and-down {
10% {
    height: 30%;
}
30% {
    height: 100%;
}
60% {
    height: 50%;
}
80% {
    height: 75%;
}
100% {
    height: 20%;
}
}
@keyframes up-and-down {
10% {
    height: 30%;
}
30% {
    height: 100%;
}
60% {
    height: 50%;
}
80% {
    height: 75%;
}
100% {
    height: 20%;
}
}

