.tb_head {
  width: 100%;
  height: 252rpx;
  text-align: center;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding-top: 30rpx;
  background: url(data:image/png;base64,) no-repeat center top;
  background-size: 100% 100%;
}
.tb_head .h3 {
    font-size: 40rpx;
    line-height: 56rpx;
    color: #242629;
    font-weight: bold;
    position: relative;
    padding-bottom: 15rpx;
    margin-bottom: 15rpx;
}
.tb_head .h3 .num {
      font-size: 56rpx;
      line-height: 56rpx;
      color: #1F68C8;
      display: inline-block;
      margin: 0 10rpx;
      vertical-align: bottom;
}
.tb_head .h3 :after {
      content: '';
      display: inline-block;
      width: 60rpx;
      height: 4rpx;
      background: #AED3FF;
      position: absolute;
      bottom: 0;
      left: 50%;
      margin-left: -30rpx;
}
.tb_head .p {
    font-size: 32rpx;
    line-height: 44rpx;
    color: #242629;
}
.tb_title {
  text-align: center;
  margin-bottom: 80rpx;
}
.tb_title .py {
    font-size: 30rpx;
    line-height: 42rpx;
    color: #777777;
}
.tb_title .zh {
    font-size: 60rpx;
    line-height: 84rpx;
    color: #4E84C8;
    font-weight: bold;
}
.tb_title .en {
    font-size: 28rpx;
    line-height: 40rpx;
    color: #88A7CE;
}
.tb_ctn {
  margin: 0 32rpx;
}
.tb_ctn .ctn_img {
    width: 100%;
}
.arrow_btm {
  width: 78rpx;
  height: 78rpx;
  text-align: center;
  border: 1rpx solid #E3E3E3;
  border-radius: 10rpx;
  background: #fff;
  position: absolute;
  bottom: 100rpx;
  left: 50%;
  margin-left: -39rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 15rpx 0;
}
.arrow_btm .ico {
    width: 36rpx;
    height: 48rpx;
}

