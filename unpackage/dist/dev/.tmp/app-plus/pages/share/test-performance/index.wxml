<view><block wx:if="{{detailData}}"><download-top vue-id="1" detailData="{{detailData}}" lan="{{lan}}" bind:__l="__l"></download-top></block><view class="custom_head"><block wx:if="{{detailData}}"><view class="star_list"><block wx:for="{{[1,2,3]}}" wx:for-item="item" wx:for-index="__i0__" wx:key="*this"><view class="{{['span',[(item<=detailData.star)?'act':'']]}}"></view></block></view></block><block wx:if="{{detailData}}"><view class="score">{{detailData.score}}</view></block><block wx:if="{{detailData}}"><text class="text">{{''+langData.sharePerformance.headText01[lan]}}<view class="_br"></view>{{''+(langData.sharePerformance.headText02[lan]+detailData.score+langData.sharePerformance.headText03[lan])+''}}</text></block><block wx:if="{{detailData}}"><view class="custom_data"><view class="money"><view class="icons ico_money _i"></view></view><view class="info"><view class="h3">{{langData.sharePerformance.totalCoinText[lan]+" "+detailData.totalCoin+" "+langData.sharePerformance.coin[lan]}}</view><view class="h4">{{langData.sharePerformance.rightTitle[lan]}}</view><view class="p">{{"+ "+detailData.rightCoin+" "+langData.sharePerformance.coin[lan]}}</view></view><view data-event-opts="{{[['tap',[['downloadFn',['$event']]]]]}}" class="btn" bindtap="__e">{{langData.shareData.challengeBtn[lan]}}</view></view></block></view><block wx:if="{{detailData&&detailData.bestParaList}}"><record-list vue-id="2" detailData="{{detailData}}" lan="{{lan}}" bind:__l="__l"></record-list></block><block wx:if="{{detailData&&(detailData.bestLexiconList||detailData.badLexiconList)}}"><view class="sczw_mdl"><block wx:if="{{detailData.lexiconLevel}}"><image class="pj_img" mode="widthFix" src="{{imgUrl+'/ico_'+detailData.lexiconLevel+'.png'}}"></image></block><view class="sczw_title"><view class="icons ico_word"></view>{{langData.sharePerformance.newWordTitle[lan]}}</view><view class="sczw_ctn"><block wx:if="{{detailData.bestLexiconList}}"><view class="bxzj_box"><view class="tit"><view class="icons ico_gou"></view><text class="text">{{langData.sharePerformance.expressionTitle[lan]}}</text></view><view class="bxzj_list"><block wx:for="{{detailData.bestLexiconList}}" wx:for-item="item" wx:for-index="__i1__" wx:key="*this"><view class="li">{{item}}</view></block></view></view></block><block wx:if="{{detailData.badLexiconList}}"><view class="xyjq_box"><view class="tit"><view class="icons ico_th"></view><text class="text">{{langData.sharePerformance.reinforceTitle[lan]}}</text></view><view class="xyjq_list"><block wx:for="{{detailData.badLexiconList}}" wx:for-item="item" wx:for-index="__i2__" wx:key="*this"><view class="li">{{item}}</view></block></view></view></block></view></view></block><block wx:if="{{detailData&&detailData.impoParaList}}"><view class="hxjz_mdl"><view class="hxjz_title"><view class="icons ico_hxjz"></view>{{langData.sharePerformance.kernelTitle[lan]}}</view><view class="hxjz_ctn"><view class="hxjz_list"><block wx:for="{{detailData.impoParaList}}" wx:for-item="item" wx:for-index="__i3__" wx:key="*this"><view class="li"><view class="dot"></view><view class="text">{{item}}</view></view></block></view><view class="zs_box"><image class="bg" src="{{imgUrl+'/textbook/q_ico.png'}}"></image><view class="zh">{{langData.sharePerformance.kernelDes[lan]}}</view></view></view></view></block><block wx:if="{{detailData}}"><download-bottom vue-id="3" detailData="{{detailData}}" lan="{{lan}}" bind:__l="__l"></download-bottom></block></view>