<block wx:if="{{detailData}}"><view class="share_record"><view class="record_title"><view class="icons ico_zan _i"></view>{{langData.shareData.bestTitle[lan]}}</view><view class="record_list"><block wx:for="{{detailData.paraList}}" wx:for-item="item" wx:for-index="index"><view class="record_item"><view class="tit">{{item.content.zh}}</view><view class="audio_list"><view class="li"><view data-event-opts="{{[['tap',[['playAudio',['user',index]]]]]}}" class="play_box" bindtap="__e"><image class="img" src="{{detailData.headImg}}" alt></image><view class="mask"></view><view class="{{['icons','ico_play',[(item.isUserPlay)?'stop':'']]}}"></view></view><view class="text">{{''+langData.shareData.recordText01[lan]+' -'}}<text class="orange">{{item.score+langData.shareData.scoreText[lan]}}</text></view></view><view class="li"><view data-event-opts="{{[['tap',[['playAudio',['para',index]]]]]}}" class="play_box" bindtap="__e"><view class="{{['icons','ico_play',[(item.isParaPlay)?'stop':'']]}}"></view><view class="progress"></view></view><view class="text">{{langData.shareData.recordText02[lan]}}</view></view></view></view></block></view></view></block>